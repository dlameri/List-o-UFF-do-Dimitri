<%= semantic_form_for [:admin, @curso] do |f| %>  
	<%= f.inputs "Incluir Um-Por-Um" do %>
  		<%= f.input :unidade %>
    	<%= f.input :nome %>
    	<%= f.input :vagas %>
    <% end %>
  	<%= f.buttons %>
<% end %>

<%= semantic_form_for [:admin, @curso], :html => { :multipart => true }  do |f| %>  
	<%= f.inputs "Incluir em massa" do %>
  		<%= f.input :txtFile, :as => :file, :label => "Arquivo" %>    	
    <% end %>
  	<%= f.buttons %>
<% end %>